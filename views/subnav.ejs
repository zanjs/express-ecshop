<% tree.forEach(function(t) { %>
    <% if(t.parent_id == 0){ %>
    <div>
        <span class="left">
            <a href="/category?cat_id=<%=t._id%>">
                <%=t.cat_name%>
            </a>
        </span>
        <span class="right subnav_right">
            <img src="/images/line.gif" border="0" id="categorie_ico1">
        </span>
    </div>
    <% } else { %>
    <ul>
        <li>&nbsp;
            <a href="/category?cat_id=<%=t._id%>">
                <%=t.cat_name%>
            </a>
        </li>
    </ul>
    <% } %>
<% }); %>